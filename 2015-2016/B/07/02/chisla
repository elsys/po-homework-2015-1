#include <stdio.h>
int main() {
    long int ezpz,a=10,b[100],i=0,c=0;
    scanf("%d", &ezpz);
        while (ezpz!=0){
            if(ezpz>=10){
                b[i] = ezpz%a;
                ezpz = (ezpz - ezpz%a)/10;
            }
            else {
                b[i] = ezpz;
                ezpz = 0;
            }
            i++;
        }
        i--;
        b[99] = i;
        a = 0;
        while (a<=9) {
            while (i>=0) {
                if(b[i] == a) c++;
                i--;
            }
            printf("%d go ima %d puti\n",a,c);
            i = b[99];
            c = 0;
            a++;
        }
return 0;
}
