#include<stdio.h>
int main()
{
  char n[1000];
  int i=0;
  do{
        scanf("\n%s",n);
        if(n[0]=='S'&&n[1]=='T'&&n[2]=='O'&&n[3]=='P')
        break;
        else
      {
        int f=0;
        for(;f<100;f++)
        {
          if(n[f]>='a'&& n[f]<='z')
        {
          n[f]-=32;
        }
         }
          printf("\n%s",n);
      }
    }while(1);
  return 0;
}
